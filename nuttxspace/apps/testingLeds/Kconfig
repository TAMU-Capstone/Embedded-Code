#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#
menu "Testing Leds"
config TEST
	tristate "\"testingLeds\" example"
	default n
	---help---
		Enable the \"testingLeds\" example

if TEST

config TEST_PROGNAME
	string "Program name"
	default "testingLeds"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config TEST_PRIORITY
	int "testingLeds task priority"
	default 100

config TEST_STACKSIZE
	int "testingLeds stack size"
	default DEFAULT_TASK_STACKSIZE

endif
endmenu # testingLeds Framework
