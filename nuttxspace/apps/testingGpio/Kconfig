menu "Testing USER_TEST_GPIO_1"
config USER_TEST_GPIO_1
	tristate "\"testingUSER_TEST_GPIO_1\" example"
	default n
	---help---
		Enable the \"testingUSER_TEST_GPIO_1\" example

if USER_TEST_GPIO_1

config USER_TEST_GPIO_1_PROGNAME
	string "Program name"
	default "testingUSER_TEST_GPIO_1"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config USER_TEST_GPIO_1_PRIORITY
	int "testingUSER_TEST_GPIO_1 task priority"
	default 100

config USER_TEST_GPIO_1_STACKSIZE
	int "testingUSER_TEST_GPIO_1 stack size"
	default DEFAULT_TASK_STACKSIZE


endif
endmenu # testingUSER_TEST_GPIO_1 Framework
