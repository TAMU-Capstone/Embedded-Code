#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#
menu "Testing GPIO"
config GPIO
	tristate "\"testingGPIO\" example"
	default n
	---help---
		Enable the \"testingGPIO\" example

if GPIO

config GPIO_PROGNAME
	string "Program name"
	default "testingGPIO"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config GPIO_PRIORITY
	int "testingGPIO task priority"
	default 100

config GPIO_STACKSIZE
	int "testingGPIO stack size"
	default DEFAULT_TASK_STACKSIZE

endif
endmenu # testingGPIO Framework
